<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <title>Fabric.js vs. Raphael Benchmarks | {{name}}</title>
  <script src="/node_modules/qunit/qunit/qunit.js"></script>
  <script src="/testem.js"></script>
  {{#serve_files}}
  <script src="{{src}}"></script>
  {{/serve_files}}
  {{#css_files}}
  <link rel="stylesheet" href="{{src}}">
  {{/css_files}}
  <script>
  QUnit.module('benchmarks / fabric vs. Raphael / {{name}}');
  </script>
</head>

<body class="px-1">
  <div>
    <h2><span>Fabric.js benchmarks</span> &middot; vs. Raphael ({{name}})</h2>
    <div id="preload" class="preload">
      {{#assets}}
      <{{tag}} src="{{src}}" onload="preload(this)"></{{tag}}>
      {{/assets}}
    </div>
    {{{pre}}}
    <div class="flex-row">
      <div class="px-1">
        <canvas id="canvas" width="600" height="600"></canvas>        
        <h2><span>fabric</span><br>
          <b id="fabric_result" class="bench"></b>
        </h2>
      </div>
      <div class="px-1">
        <div id="raphael"></div>
        <h2><span>Raphael</span><br>
          <b id="raphael_result" class="bench"></b>
        </h2>
      </div>
    </div>
</body>

</html>